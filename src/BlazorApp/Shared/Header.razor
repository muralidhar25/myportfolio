    <div id="headerResume">
    <img src="images/download_icon.png" />
     <a style="float:right" href="@(resume.ResumePath)" title="Download Resume" download>
        <span>Download Resume</span></a>
    </div>
@code{
     [Parameter, EditorRequired]
    public required HttpClient Http { get; set; }

    private Resume? resume;
    protected override async Task OnInitializedAsync()
    {
        resume = await Http.GetFromJsonAsync<Resume>("sample-data/resume.json");
    }
}